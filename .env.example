# This file is an example. Copy it to .env and fill in your actual values.
# Do not commit the .env file to version control.

# A secret key for signing and authentication. Keep this private.
# Generate a secure key using: openssl rand -hex 32
PHOTOSHARE_API_KEY=your_secret_api_key_here

# Your Google Maps API key for reverse geocoding in the slideshow.
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Comma-separated list of directories to scan for photos.
# e.g., PHOTOSHARE_PHOTO_DIRS=/path/to/your/photos,/another/path/to/photos
PHOTOSHARE_PHOTO_DIRS=./sample_photos

# The path to the SQLite database file.
PHOTOSHARE_DATABASE_FILE=photoshare.db
